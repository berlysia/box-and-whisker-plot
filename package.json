    "react-dom": "^17.0.2",
    "react-vis": "^1.11.7"
  },
  "devDependencies": {
    "@berlysia/eslint-config": "^2.1.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
