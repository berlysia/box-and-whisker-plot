{
  "name": "box-and-whisker-plot",
  "version": "0.0.0",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint '**/*.{js,ts,jsx,tsx}'",
